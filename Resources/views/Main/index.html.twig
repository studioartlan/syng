<!doctype html>
<html lang="en" ng-app="{{ngAppConfig.name}}">
<head>
  <meta charset="utf-8">
  <title>My AngularJS App</title>

{% stylesheets
	'@StudioArtlanSngDemoBundle/Resources/public/app/css/app.css'
	filter='cssrewrite'
%}
    <link rel="stylesheet" href="{{ asset_url }}" />
{% endstylesheets %}

</head>
<body>
  <ul class="menu">
    <li><a href="#/view1">view1</a></li>
    <li><a href="#/view2">view2</a></li>
  </ul>

  <div ng-view></div>

  <div>Angular seed app: v<span app-version></span></div>


{% javascripts
	'@StudioArtlanSngDemoBundle/Resources/public/app/lib/angular.js'
	'@StudioArtlanSngDemoBundle/Resources/public/app/lib/angular-route.js'
    '@StudioArtlanSngDemoBundle/Resources/public/app/js/*.js'
    '@StudioArtlanSngDemoBundle/Resources/public/app/js/routes/*.js'
    '@StudioArtlanSngDemoBundle/Resources/public/app/js/services/*.js'
    '@StudioArtlanSngDemoBundle/Resources/public/app/js/controllers/*.js'
    '@StudioArtlanSngDemoBundle/Resources/public/app/js/filters/*.js'
    '@StudioArtlanSngDemoBundle/Resources/public/app/js/directives/*.js'
%}
<script type="text/javascript" src="{{ asset_url }}"></script>
{% endjavascripts %}

</body>
</html>
